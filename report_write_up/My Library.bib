@article{alaux2011a,
  title = {Nutrigenomics in Honey Bees: Digital Gene Expression Analysis of Pollen's Nutritive Effects on Healthy and Varroa-Parasitized Bees},
  shorttitle = {Nutrigenomics in Honey Bees},
  author = {Alaux, Cédric and Dantec, Christelle and Parrinello, Hughes and Le Conte, Yves},
  date = {2011-10-10},
  journaltitle = {BMC Genomics},
  shortjournal = {BMC Genomics},
  volume = {12},
