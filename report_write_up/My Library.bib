}

@article{barbosa2015,
  title = {Lethal and Sublethal Effects of Azadirachtin on the Bumblebee {{Bombus}} Terrestris ({{Hymenoptera}}: {{Apidae}})},
  shorttitle = {Lethal and Sublethal Effects of Azadirachtin on the Bumblebee {{Bombus}} Terrestris ({{Hymenoptera}}},
