@article{petrovici2020,
  title = {Toxicity of {{Deltamethrin}} to {{Zebrafish Gonads Revealed}} by {{Cellular Biomarkers}}},
  author = {Petrovici, Adriana and Strungaru, Stefan-Adrian and Nicoara, Mircea and Robea, Madalina Andreea and Solcan, Carmen and Faggio, Caterina},
  date = {2020-02},
  journaltitle = {Journal of Marine Science and Engineering},
  volume = {8},
