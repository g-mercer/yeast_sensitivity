  issn = {2399-3642},
  doi = {10.1038/s42003-021-02336-2},
  url = {https://www.nature.com/articles/s42003-021-02336-2},
  urldate = {2021-07-16},
  abstract = {The assessment of pesticide risks to insect pollinators have typically focused on short-term, lethal impacts. The environmental ramifications of many of the world’s most commonly employed pesticides, such as those exhibiting systemic properties that can result in long-lasting exposure to insects, may thus be severely underestimated. Here, seven laboratories from Europe and North America performed a standardised experiment (a ring-test) to study the long-term lethal and sublethal impacts of the relatively recently approved ‘bee safe’ butenolide pesticide flupyradifurone (FPF, active ingredient in Sivanto®) on honey bees. The emerging contaminant, FPF, impaired bee survival and behaviour at field-realistic doses (down to 11\,ng/bee/day, corresponding to 400\,µg/kg) that were up to 101-fold lower than those reported by risk assessments (1110\,ng/bee/day), despite an absence of time-reinforced toxicity. Our findings raise concerns about the chronic impact of pesticides on pollinators at a global scale and support a novel methodology for a refined risk assessment.},
  issue = {1},
  langid = {english},
  annotation = {Bandiera\_abtest: a Cc\_license\_type: cc\_by Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Agriculture;Animal behaviour;Drug regulation;Entomology;Toxicology Subject\_term\_id: agriculture;animal-behaviour;drug-regulation;entomology;toxicology},
  file = {/Users/guy/Zotero/storage/UBLNJRH5/Tosi et al. - 2021 - Long-term field-realistic exposure to a next-gener.pdf;/Users/guy/Zotero/storage/9476HRT5/s42003-021-02336-2.html}
}

@article{toussaint2006,
  title = {High-Throughput and Sensitive Assay to Measure Yeast Cell Growth: A Bench Protocol for Testing Genotoxic Agents},
  shorttitle = {High-Throughput and Sensitive Assay to Measure Yeast Cell Growth},
  author = {Toussaint, Martin and Conconi, Antonio},
