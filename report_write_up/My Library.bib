  keywords = {Deltamethrin,Lymphoid organs,Oxidative stress,Polymorphonuclear leukocytes,Rat,Thiacloprid},
  file = {/Users/guy/Zotero/storage/AYVA63CK/Aydin - 2011 - Effects of thiacloprid, deltamethrin and their com.pdf;/Users/guy/Zotero/storage/H2AADSIF/S0048357511000617.html}
}

