}

@article{pickett2017,
  title = {Acetylcholinesterase Plays a Non-Neuronal, Non-Esterase Role in Organogenesis},
  author = {Pickett, Melissa A. and Dush, Michael K. and Nascone-Yoder, Nanette M.},
  date = {2017-08-01},
  journaltitle = {Development (Cambridge, England)},
  shortjournal = {Development},
  volume = {144},
