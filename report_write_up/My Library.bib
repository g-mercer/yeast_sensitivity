@article{whitehorn2012,
  title = {Neonicotinoid {{Pesticide Reduces Bumble Bee Colony Growth}} and {{Queen Production}}},
  author = {Whitehorn, Penelope R. and O’Connor, Stephanie and Wackers, Felix L. and Goulson, Dave},
  date = {2012-04-20},
  journaltitle = {Science},
  volume = {336},
