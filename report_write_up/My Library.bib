@article{tavares2015,
  title = {In Vitro Effects of Thiamethoxam on Larvae of {{Africanized}} Honey Bee {{Apis}} Mellifera ({{Hymenoptera}}: {{Apidae}})},
  shorttitle = {In Vitro Effects of Thiamethoxam on Larvae of {{Africanized}} Honey Bee {{Apis}} Mellifera ({{Hymenoptera}}},
