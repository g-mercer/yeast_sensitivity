  title = {Chapter 2 - {{Pollution}} Status and Biodegradation of Organophosphate Pesticides in the Environment},
  booktitle = {Abatement of {{Environmental Pollutants}}},
  author = {Dar, Mohd Ashraf and Kaushik, Garima and Villareal Chiu, Juan Francisco},
