@article{claus2019,
  title = {Protein-Facilitated Transport of Hydrophobic Molecules across the Yeast Plasma Membrane},
  author = {Claus, Silke and Jezierska, Sylwia and Bogaert, Inge N. A. Van},
  date = {2019},
  journaltitle = {FEBS Letters},
