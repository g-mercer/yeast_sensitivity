@article{morfin2020,
  title = {Interaction of {{Varroa}} Destructor and {{Sublethal Clothianidin Doses}} during the {{Larval Stage}} on {{Subsequent Adult Honey Bee}} ({{Apis}} Mellifera {{L}}.) {{Health}}, {{Cellular Immunity}}, {{Deformed Wing Virus Levels}} and {{Differential Gene Expression}}},
  author = {Morfin, Nuria and Goodwin, Paul H. and Guzman-Novoa, Ernesto},
