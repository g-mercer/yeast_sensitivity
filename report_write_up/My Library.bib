  langid = {english},
  file = {/Users/guy/Zotero/storage/Q4QSZAQ2/Kleijn et al. - 2015 - Delivery of crop pollination services is an insuff.pdf;/Users/guy/Zotero/storage/YYZ46YQ4/ncomms8414.html}
}

