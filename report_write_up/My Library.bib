@article{benzidane2011,
  title = {Neonicotinoid Insecticides Imidacloprid and Clothianidin Affect Differently Neural {{Kenyon}} Cell Death in the Cockroach {{Periplaneta}} Americana},
  author = {Benzidane, Yassine and Lapied, Bruno and Thany, Steeve H.},
  date = {2011-11-01},
  journaltitle = {Pesticide Biochemistry and Physiology},
  shortjournal = {Pesticide Biochemistry and Physiology},
  volume = {101},
