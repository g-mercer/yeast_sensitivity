  title = {The {{Effects}} of {{Exposure}} to {{Flupyradifurone}} on {{Survival}}, {{Development}}, and {{Foraging Activity}} of {{Honey Bees}} ({{Apis}} Mellifera {{L}}.) under {{Field Conditions}}},
  author = {Guo, Yi and Diao, Qing-Yun and Dai, Ping-Li and Wang, Qiang and Hou, Chun-Sheng and Liu, Yong-Jun and Zhang, Li and Luo, Qi-Hua and Wu, Yan-Yan and Gao, Jing},
