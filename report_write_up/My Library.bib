@article{katzmann1994,
  title = {Transcriptional Control of the Yeast {{PDR5}} Gene by the {{PDR3}} Gene Product.},
  author = {Katzmann, D J and Burnett, P E and Golin, J and Mahé, Y and Moye-Rowley, W S},
  date = {1994-07},
  journaltitle = {Molecular and Cellular Biology},
