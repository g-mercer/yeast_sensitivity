@article{delaveau1994,
  title = {{{PDR3}}, a New Yeast Regulatory Gene, Is Homologous {{toPDR1}} and Controls the Multidrug Resistance Phenomenon},
  author = {Delaveau, Thierry and Delahodde, Agnès and Carvajal, Elvira and Subik, Julius and Jacq, Claude},
  date = {1994-09-01},
