  author = {Stanley E. Lazic and Charlie J. Clarke-Williams and Marcus R. Munaf{\a`o}},
  date = {2018-04-04},
  journaltitle = {PLOS Biology},
  shortjournal = {PLOS Biology},
  volume = {16},
