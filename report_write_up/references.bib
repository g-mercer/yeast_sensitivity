  file = {/Users/guy/Zotero/storage/7ZZFLCCK/Mossa et al. - 2013 - Amelioration of Prallethrin-Induced Oxidative Stre.pdf},
  pmcid = {PMC3870102},
}

@Article{nasuti2003,
  title = {Different Effects of {{Type I}} and {{Type II}} Pyrethroids on Erythrocyte Plasma Membrane Properties and Enzymatic Activity in Rats},
  author = {Cinzia Nasuti and Franco Cantalamessa and Giancarlo Falcioni and Rosita Gabbianelli},
  date = {2003-09-30},
  journaltitle = {Toxicology},
  shortjournal = {Toxicology},
  volume = {191},
  pages = {233--244},
  issn = {0300-483X},
