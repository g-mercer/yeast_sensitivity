@Article{gabbianelli2004,
  title = {Lymphocyte {{DNA}} Damage in Rats Exposed to Pyrethroids: Effect of Supplementation with {{Vitamins E}} and {{C}}},
  shorttitle = {Lymphocyte {{DNA}} Damage in Rats Exposed to Pyrethroids},
  author = {Rosita Gabbianelli and Cinzia Nasuti and Giancarlo Falcioni and Franco Cantalamessa},
  date = {2004-10-15},
  journaltitle = {Toxicology},
  shortjournal = {Toxicology},
  volume = {203},
  pages = {17--26},
  issn = {0300-483X},
