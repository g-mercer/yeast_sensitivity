  date = {2019-12-01},
  journaltitle = {Toxicology in Vitro},
  shortjournal = {Toxicology in Vitro},
  volume = {61},
  pages = {104654},
  issn = {0887-2333},
  doi = {10.1016/j.tiv.2019.104654},
